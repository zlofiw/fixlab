{
  "name": "fixlab-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/web",
    "apps/api"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "npm run dev --workspace @fixlab/web",
    "dev:api": "npm run start:dev --workspace @fixlab/api",
    "build": "npm run build:web && npm run build:api",
    "build:web": "npm run build --workspace @fixlab/web",
    "build:api": "npm run build --workspace @fixlab/api",
    "preview:web": "npm run preview --workspace @fixlab/web",
    "lint:web": "npm run lint --workspace @fixlab/web"
  }
}
